{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "json": {
      "()": "app.infrastructure.logging.json_formatter.JsonLogFormatter"
    }
  },
  "handlers": {
    "default": {
      "class": "logging.StreamHandler",
      "formatter": "json",
      "stream": "ext://sys.stdout"
    }
  },
  "loggers": {
    "uvicorn": {
      "handlers": ["default"],
      "level": "INFO",
      "propagate": false
    },
    "uvicorn.error": {
      "handlers": ["default"],
      "level": "INFO",
      "propagate": false
    },
    "uvicorn.access": {
      "handlers": ["default"],
      "level": "INFO",
      "propagate": false
    },
    "app": {
      "handlers": ["default"],
      "level": "INFO",
      "propagate": false
    }
  },
  "root": {
    "handlers": ["default"],
    "level": "INFO"
  }
}
